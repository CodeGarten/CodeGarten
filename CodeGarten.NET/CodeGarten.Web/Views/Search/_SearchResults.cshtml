@model IEnumerable<object>
@{
    var genericType = Model.GetType().GetGenericArguments().First();
    var genericTypeName = genericType.Name;
}
@{
    var sufix = Model.Count() == 1 ? "" : "s";
}
<h3>@Model.Count() @genericTypeName@sufix found</h3>
@if (Model.Count() > 0)
{
    switch (genericTypeName)
    {
        case "User":
            {
                foreach (var entity in Model)
                {
    @Html.Partial("_UserItem", entity)
                }
                break;
            }
        case "Structure":
            {
                foreach (var entity in Model)
                {
    @Html.Partial("_StructureItem", entity)
                }
                break;
            }
            case "Container":
            {
                foreach (var entity in Model)
                {
    @Html.Partial("_ContainerItem", entity)
                }
                break;
            }
    }
}