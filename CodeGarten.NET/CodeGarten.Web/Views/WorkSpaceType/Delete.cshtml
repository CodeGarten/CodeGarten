@model CodeGarten.Data.Model.WorkSpaceType
<h3>
    Are you sure you want to delete this?</h3>
<h2>
    Delete workspace type</h2>
<div class="display-label">
    Name</div>
<div class="display-field">@Model.Name</div>
<div>
    <h3>
        Services</h3>
    @foreach (var service in Model.Services)
    {
        <div class="display-field">@service.Name</div>
    }
</div>
@using (Ajax.BeginForm(new AjaxOptions { OnSuccess = "DialogHelper.OnSuccess", Url = "/WorkSpaceType/Delete?structureId=" + Request["structureId"] + "&name=" + Model.Name }))
{ }
